{
  "name": "harekrishna-app",
  "displayName": "Hare Krishna",
  "description": "The devotees of Lord Viṣṇu are godly and Association of Devotees is one of the most important activity in Bhakti.\nThis portal is a digital initiative towards facilitating this.",
  "version": "1.0.0",
  "private": true,
  "author": "Vivek Chandra",
  "scripts": {
    "dev": "prisma generate --accelerate --schema './src/prisma/schema.prisma' && next dev --turbopack",
    "build": "prisma generate --accelerate --schema './src/prisma/schema.prisma' && next build",
    "start": "next start",
    "prod": "pnpm build && pnpm start",
    "lint": "next lint"
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "@prisma/extension-accelerate": "^2.0.1",
    "axios": "^1.9.0",
    "chart.js": "^4.5.0",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "formik": "^2.4.6",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "next": "15.3.8",
    "primeicons": "^7.0.0",
    "primereact": "^10.9.5",
    "qrcode.react": "^4.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.7",
    "xlsx": "^0.18.5",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
